<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mascitra.android.pengaduanlmj">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:name=".config.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcer"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcer_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.DashboardActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity android:name=".activity.DashboardActivity" /> -->
        <activity android:name=".activity.DetailPengaduanActivity"/>
        <activity android:name=".activity.AboutUsActivity" />
        <activity android:name=".activity.FormulirPengaduanActivity" />
        <activity android:name=".activity.DetailBiodataActivity" />
        <activity android:name=".activity.FormulirActivity" />
        <activity android:name=".activity.LihatPengaduanActivity" />
        <activity android:name=".activity.PanduanActivity" />
        <activity android:name=".activity.RevisiActivity" />

    </application>

</manifest>